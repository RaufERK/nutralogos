# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
OPENAI_CHAT_MODEL=gpt-4o
USE_MOCK=false

# Qdrant Configuration
QDRANT_URL=your_qdrant_url_here
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_COLLECTION_NAME=nutralogos

# NextAuth.js Configuration
NEXTAUTH_SECRET=your-super-secret-key-here
NEXTAUTH_URL=http://localhost:3000

ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=change_me

# WebSocket Configuration for Streaming
ENABLE_STREAMING=true
WEBSOCKET_PORT=3001
WEBSOCKET_HTTP_PORT=3002
ALLOWED_ORIGINS=http://localhost:3000,https://nutralogos.ru,https://www.nutralogos.ru

# Env folder info
ENV_PATH=/WEB_PROJ/nutralogos/.env
DATA_DIR=./data
